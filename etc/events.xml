<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Event/etc/events.xsd">
    <event name="payment_method_assign_data_thevaultapp">
        <observer name="thevaultapp_gateway_data_assign" instance="TheVaultApp\Magento2\Observer\DataAssignObserver" />
    </event>
    <event name="email_order_set_template_vars_before">
        <observer name="add_extra_data_to_transport" instance="TheVaultApp\Magento2\Observer\AddExtraDataToTransport" />
    </event>
	<event name="order_cancel_after">
        <observer name="update_hub_order_cancel" instance="TheVaultApp\Magento2\Observer\OrderCancelObserver" />
    </event>    
   <event name="sales_order_place_after">
       <observer name="actions_on_order_placed" instance="TheVaultApp\Magento2\Observer\AfterPlaceOrderObserver" />
   </event>
</config>
